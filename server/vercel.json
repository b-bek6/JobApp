{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "timeout":[{
        "functions": {
          "api/**/*.js": {
            "timeout": 30
          }
        }
    }]
    }
